{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && npm run prettier -s",
    "nx": "nx",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint && stylelint \"**/*.scss\" --syntax scss && htmlhint \"apps\" --config .htmlhintrc",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "prettier": "prettier --write \"./apps/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./apps/**/*.{ts,js,html,scss}\"",
    "help": "nx help",
    "compodoc": "./node_modules/.bin/compodoc -c .compodocrc.json",
    "dev:ssr": "ng run angular-boilerplate:serve-ssr",
    "serve:ssr": "node dist/angular-boilerplate/server/main.js",
    "build:ssr": "ng build --prod && ng run angular-boilerplate:server:production",
    "prerender": "ng run angular-boilerplate:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/platform-server": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@nguniversal/express-engine": "^9.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/angular": "^9.5.1",
    "express": "^4.15.2",
    "lint-staged": "^10.2.11",
    "ngx-cookie-service": "^3.0.4",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.4",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@compodoc/compodoc": "^1.1.11",
    "@nguniversal/builders": "^9.1.1",
    "@nrwl/cypress": "9.5.1",
    "@nrwl/jest": "9.5.1",
    "@nrwl/workspace": "9.5.1",
    "@types/express": "^4.17.0",
    "@types/jest": "25.1.4",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "htmlhint": "^0.14.1",
    "husky": "^4.2.5",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged && npm run test",
      "pre-push": "npm run lint && pretty-quick --staged && npm run test"
    }
  }
}
